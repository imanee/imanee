{
    "name": "imanee/imanee",
    "description": "PHP Image Manipulation Library",
    "keywords": ["image", "php", "imagick", "gd", "imagemagick"],
    "homepage": "http://imanee.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Erika Heidi",
            "email": "erika@erikaheidi.com"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "ext-imagick": "*",
        "ext-gd": "*",
        "mockery/mockery": "^0.9.4",
        "satooshi/php-coveralls": "^0.6.1",
        "raveren/kint":"^1.0"
    },
    "suggest": {
        "ext-imagick": "To allow Imanee\\ImageResource\\ImagickResource usage.",
        "ext-gd": "To allow Imanee\\ImageResource\\GDResource usage."
    },
    "autoload": {
        "psr-4": { "Imanee\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Imanee\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
